gem "jekyll-feed"
gem "jekyll-sitemap"
gem "jekyll-include-cache"
gem "jekyll-remote-theme"
gem "jekyll-seo-tag"
gem "jekyll-paginate"
gem "jekyll-relative-links"
gem "jekyll-readme-index"
